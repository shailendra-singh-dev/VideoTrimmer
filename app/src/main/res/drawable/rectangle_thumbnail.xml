    </item>
    <item
        android:bottom="10dp"
        android:left="9dp"
        android:right="9dp"
        android:top="10dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white" />
        </shape>
    </item>


</layer-list>


